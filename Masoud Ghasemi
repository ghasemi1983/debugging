def id_to_fruit(fruit_id: int, fruits: Set[str]) -> str:
    fruit_list = sorted(fruits)  # Convert set to a sorted list
    if 0 <= fruit_id < len(fruit_list):
        return fruit_list[fruit_id]
    raise RuntimeError(f"Fruit with id {fruit_id} does not exist")
def swap(coords: np.ndarray) -> np.ndarray:
    swapped = coords.copy()  # Avoid modifying the original array
    swapped[:, [0, 1, 2, 3]] = swapped[:, [1, 0, 3, 2]]  # Swap correctly
    return swapped
def plot_data(csv_file_path: str):
    results = []
    with open(csv_file_path) as result_csv:
        csv_reader = csv.reader(result_csv, delimiter=",")
        next(csv_reader)  # Skip header
        for row in csv_reader:
            results.append([float(row[0]), float(row[1])])  # Convert to float
    results = np.array(results)

    # Swap indices to correctly plot precision vs recall
    plt.plot(results[:, 0], results[:, 1])  # Precision on x-axis, Recall on y-axis
    plt.ylim([-0.05, 1.05])
    plt.xlim([-0.05, 1.05])
    plt.xlabel("Precision")
    plt.ylabel("Recall")
    plt.show()
